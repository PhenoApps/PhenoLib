<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"

    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="10dp">

    <EditText
        android:id="@+id/studiesSeasonsGetSeasonDbIdEditText"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="seasonDbId"
        android:inputType="number"
        android:maxLines="1"

        tools:ignore="Autofill, HardcodedText" />

    <EditText
        android:id="@+id/studiesSeasonsGetSeasonEditText"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="season"
        android:inputType="text"
        android:maxLines="1"

        tools:ignore="Autofill, HardcodedText" />

    <EditText
        android:id="@+id/studiesSeasonsGetYearEditText"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="year"
        android:inputType="number"
        android:maxLines="1"

        tools:ignore="Autofill, HardcodedText" />

    <EditText
        android:id="@+id/studiesSeasonsGetPageEditText"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="page"
        android:inputType="number"
        android:maxLines="1"

        tools:ignore="Autofill, HardcodedText" />

    <EditText
        android:id="@+id/studiesSeasonsGetPageSizeEditText"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="page size"
        android:inputType="number"
        android:maxLines="1"

        tools:ignore="Autofill, HardcodedText" />

</LinearLayout>