<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"

    android:layout_width="match_parent"
    android:layout_height="match_parent"

    tools:context=".brapi1_3.TrialsFragment">



    <TextView
        android:id="@+id/trialsCommonCropNameTextView"

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_marginStart="@dimen/activity_horizontal_margin"
        android:layout_marginEnd="@dimen/activity_horizontal_margin"

        android:labelFor="@id/trialsCommonCropNameEditText"
        android:text="commonCropName:"

        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"

        tools:ignore="HardcodedText" />

    <EditText
        android:id="@+id/trialsCommonCropNameEditText"

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_marginStart="@dimen/activity_horizontal_margin"
        android:layout_marginEnd="@dimen/activity_horizontal_margin"

        android:ems="8"
        android:hint="commonCropName"
        android:inputType="text"

        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/trialsCommonCropNameTextView"

        tools:ignore="Autofill, HardcodedText" />



    <TextView
        android:id="@+id/trialsProgramDbIdTextView"

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_marginStart="@dimen/activity_horizontal_margin"
        android:layout_marginEnd="@dimen/activity_horizontal_margin"

        android:labelFor="@id/trialsProgramDbIdEditText"
        android:text="programDbId:"

        app:layout_constraintLeft_toRightOf="@id/trialsCommonCropNameEditText"
        app:layout_constraintTop_toTopOf="parent"

        tools:ignore="HardcodedText" />

    <EditText
        android:id="@+id/trialsProgramDbIdEditText"

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_marginStart="@dimen/activity_horizontal_margin"
        android:layout_marginEnd="@dimen/activity_horizontal_margin"

        android:ems="8"
        android:hint="programDbId"
        android:inputType="number"

        app:layout_constraintLeft_toRightOf="@id/trialsCommonCropNameEditText"
        app:layout_constraintTop_toBottomOf="@id/trialsProgramDbIdTextView"

        tools:ignore="Autofill, HardcodedText" />



    <TextView
        android:id="@+id/trialsLocationDbIdTextView"

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_marginStart="@dimen/activity_horizontal_margin"
        android:layout_marginEnd="@dimen/activity_horizontal_margin"

        android:labelFor="@id/trialsLocationDbIdEditText"
        android:text="locationDbId:"

        app:layout_constraintLeft_toRightOf="@id/trialsProgramDbIdEditText"
        app:layout_constraintTop_toTopOf="parent"

        tools:ignore="HardcodedText" />

    <EditText
        android:id="@+id/trialsLocationDbIdEditText"

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_marginStart="@dimen/activity_horizontal_margin"
        android:layout_marginEnd="@dimen/activity_horizontal_margin"

        android:ems="8"
        android:hint="locationDbId"
        android:inputType="number"

        app:layout_constraintLeft_toRightOf="@id/trialsProgramDbIdEditText"
        app:layout_constraintTop_toBottomOf="@id/trialsLocationDbIdTextView"

        tools:ignore="Autofill, HardcodedText" />



    <TextView
        android:id="@+id/trialsActiveTextView"

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_marginStart="@dimen/activity_horizontal_margin"
        android:layout_marginEnd="@dimen/activity_horizontal_margin"

        android:labelFor="@id/trialsActiveRadioGroup"
        android:text="active:"

        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/trialsCommonCropNameEditText"

        tools:ignore="HardcodedText" />

    <RadioGroup
        android:id="@+id/trialsActiveRadioGroup"

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_marginStart="@dimen/activity_horizontal_margin"
        android:layout_marginEnd="@dimen/activity_horizontal_margin"

        android:orientation="horizontal"

        android:checkedButton="@id/trialsNullActiveRadioButton"

        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/trialsActiveTextView">

        <RadioButton
            android:id="@+id/trialsNullActiveRadioButton"

            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:text="null"

            tools:ignore="HardcodedText" />

        <RadioButton
            android:id="@+id/trialsFalseActiveRadioButton"

            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:text="false"

            tools:ignore="HardcodedText" />

        <RadioButton
            android:id="@+id/trialsTrueActiveRadioButton"

            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:text="true"

            tools:ignore="HardcodedText" />

    </RadioGroup>



    <TextView
        android:id="@+id/trialsSortByTextView"

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_marginStart="@dimen/activity_horizontal_margin"
        android:layout_marginEnd="@dimen/activity_horizontal_margin"

        android:labelFor="@id/trialsSortByEditText"
        android:text="sortBy:"

        app:layout_constraintLeft_toRightOf="@id/trialsActiveRadioGroup"
        app:layout_constraintTop_toBottomOf="@id/trialsCommonCropNameEditText"

        tools:ignore="HardcodedText" />

    <EditText
        android:id="@+id/trialsSortByEditText"

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_marginStart="@dimen/activity_horizontal_margin"
        android:layout_marginEnd="@dimen/activity_horizontal_margin"

        android:ems="8"
        android:hint="sortBy"
        android:inputType="text"

        app:layout_constraintLeft_toRightOf="@id/trialsActiveRadioGroup"
        app:layout_constraintTop_toBottomOf="@id/trialsSortByTextView"

        tools:ignore="Autofill, HardcodedText" />



    <TextView
        android:id="@+id/trialsSortOrderTextView"

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_marginStart="@dimen/activity_horizontal_margin"
        android:layout_marginEnd="@dimen/activity_horizontal_margin"

        android:labelFor="@id/trialsSortOrderEditText"
        android:text="sortOrder:"

        app:layout_constraintLeft_toRightOf="@id/trialsSortByEditText"
        app:layout_constraintTop_toBottomOf="@id/trialsCommonCropNameEditText"

        tools:ignore="HardcodedText" />

    <EditText
        android:id="@+id/trialsSortOrderEditText"

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_marginStart="@dimen/activity_horizontal_margin"
        android:layout_marginEnd="@dimen/activity_horizontal_margin"

        android:ems="8"
        android:hint="sortOrder"
        android:inputType="text"

        app:layout_constraintLeft_toRightOf="@id/trialsSortByEditText"
        app:layout_constraintTop_toBottomOf="@id/trialsSortOrderTextView"

        tools:ignore="Autofill, HardcodedText" />



    <TextView
        android:id="@+id/trialsPageTextView"

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_marginStart="@dimen/activity_horizontal_margin"
        android:layout_marginEnd="@dimen/activity_horizontal_margin"

        android:labelFor="@id/trialsPageEditText"
        android:text="page:"

        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/trialsSortByEditText"

        tools:ignore="HardcodedText" />

    <EditText
        android:id="@+id/trialsPageEditText"

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_marginStart="@dimen/activity_horizontal_margin"
        android:layout_marginEnd="@dimen/activity_horizontal_margin"

        android:ems="8"
        android:hint="default 0"
        android:inputType="number"

        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/trialsPageTextView"

        tools:ignore="Autofill, HardcodedText" />



    <TextView
        android:id="@+id/trialsPageSizeTextView"

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_marginStart="@dimen/activity_horizontal_margin"
        android:layout_marginEnd="@dimen/activity_horizontal_margin"

        android:labelFor="@id/trialsPageSizeEditText"
        android:text="pageSize:"

        app:layout_constraintLeft_toRightOf="@id/trialsPageEditText"
        app:layout_constraintTop_toBottomOf="@id/trialsSortByEditText"

        tools:ignore="HardcodedText" />

    <EditText
        android:id="@+id/trialsPageSizeEditText"

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_marginStart="@dimen/activity_horizontal_margin"
        android:layout_marginEnd="@dimen/activity_horizontal_margin"

        android:ems="8"
        android:hint="default 1000"
        android:inputType="number"

        app:layout_constraintLeft_toRightOf="@id/trialsPageEditText"
        app:layout_constraintTop_toBottomOf="@id/trialsPageSizeTextView"

        tools:ignore="Autofill, HardcodedText" />



    <Button
        android:id="@+id/trialsGetButton"

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_marginStart="@dimen/activity_horizontal_margin"
        android:layout_marginEnd="@dimen/activity_horizontal_margin"

        android:text="trialsGet()"
        android:textAllCaps="false"

        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="@id/trialsPageEditText"

        tools:ignore="HardcodedText" />



    <TextView
        android:id="@+id/trialsTrialDbIdTextView"

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_marginStart="@dimen/activity_horizontal_margin"
        android:layout_marginEnd="@dimen/activity_horizontal_margin"

        android:labelFor="@id/trialsTrialDbIdEditText"
        android:text="trialDbId:"

        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/trialsPageEditText"

        tools:ignore="HardcodedText" />

    <EditText
        android:id="@+id/trialsTrialDbIdEditText"

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_marginStart="@dimen/activity_horizontal_margin"
        android:layout_marginEnd="@dimen/activity_horizontal_margin"

        android:ems="12"
        android:hint="required trialDbId"
        android:inputType="number"

        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/trialsTrialDbIdTextView"

        tools:ignore="Autofill, HardcodedText" />



    <Button
        android:id="@+id/trialsTrialDbIdGetButton"

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_marginStart="@dimen/activity_horizontal_margin"
        android:layout_marginEnd="@dimen/activity_horizontal_margin"

        android:text="trialsTrialDbIdGet()"
        android:textAllCaps="false"

        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="@id/trialsTrialDbIdEditText"

        tools:ignore="HardcodedText" />



    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="0dp"

        app:layout_constraintTop_toBottomOf="@id/trialsTrialDbIdEditText"
        app:layout_constraintBottom_toBottomOf="parent"

        android:layout_marginStart="@dimen/activity_horizontal_margin"
        android:layout_marginEnd="@dimen/activity_horizontal_margin">

        <TextView
            android:id="@+id/trialsResponseTextView"

            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:paddingBottom="@dimen/responseTextView_bottom_padding"

            app:fontFamily="monospace" />

    </ScrollView>



</android.support.constraint.ConstraintLayout>